#!/bin/sh
#
yyyymmdd=$1
#
PROG=fixpaobs
#	if [ $yyyymmdd -ge 19800101 -a $yyyymmdd -le 19941231 ] ; then
#
if [ $yyyymmdd -ge 19950101 -a $yyyymmdd -le 19941231 ] ; then
	rm prepfixpaobs 2>/dev/null
	if [ @MACHINE@ = cray -o @MACHINE@ = t90 -o @MACHINE@ = t3e ] ; then
		assign -R
		assign -a out.bufr     -Fcos @FTNID@20
		assign -a prepfixpaobs -Fcos @FTNID@50
	else
		rm -f @FTNID@[0-9]* 2>/dev/null
		ln -fs out.bufr      @FTNID@20
		ln -fs prepfixpaobs  @FTNID@50
	fi
	if [ @MACHINE@ = cray -o @MACHINE@ = t90 -o @MACHINE@ = t3e ] ; then
		NCPUS=1
		export NCPUS
	fi
	rm -f $PROG.x
  ln -fs @SRCS_DIR@/bin/${PROG}.x $PROG.x
  ./$PROG.x 1>$PROG.out 2>&1
  if [ $? -ne 0 ] ; then
    echo "`date` $0: Execution failed." >>ERROR.out
    exit 8
  fi
	if [ @MACHINE@ != cray -a @MACHINE@ != t90 -o @MACHINE@ != t3e ] ; then
		rm -f @FTNID@[0-9]* 2>/dev/null
	fi
	cp prepfixpaobs out.bufr
else
	echo $PROG execution skipped >>ERROR.out
fi
